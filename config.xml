<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>F.App.S.T</name>
    <description>Tactical scpace tactics games</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">FAST
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-vibration" spec="^3.0.1" />
    <plugin name="cordova-plugin-camera" spec="^4.0.1" />
    <plugin name="cordova-plugin-camerapicturebackground" spec="https://github.com/an-rahulpandey/cordova-plugin-camerapicturebackground.git" />
    <preference name="windows-target-version" value="10.0" />
    <vs:toolsetVersion xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">6.3.1</vs:toolsetVersion>
    <preference name="android-minSdkVersion" value="17" />
    <preference name="android-maxSdkVersion" value="25" />
    <preference name="android-targetSdkVersion" value="25" />
    <plugin name="cordova-plugin-media-capture" spec="^3.0.1" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="4.2.0" />
    <engine name="windows" spec="4.4.2" />
</widget>
